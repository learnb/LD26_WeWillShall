package net.umc.ludumdare.sprites;

import net.umc.ludumdare.common.Projectile;
import net.umc.ludumdare.tools.ResourceManager;

public class RedShip extends Projectile {
	
	public RedShip(int x, int y) {
		//super();
		image = ResourceManager.getImage("redship");
		velY = -5;
		this.x = x;
		this.y = y;
		width = height = 64;
	}
}
