package net.umc.ludumdare.sprites;

import net.umc.ludumdare.common.Projectile;
import net.umc.ludumdare.tools.ResourceManager;

public class YellowShip extends Projectile {
	
	public YellowShip(int x, int y) {
		//super();
		image = ResourceManager.getImage("yellowship");
		velX = -5;
		velY = -5;
		this.x = x;
		this.y = y;
	}
}
